penalty_max=3
point={}
point.new=function()
	local obj={}
	obj.score=0
	obj.penalty=0

	obj.add_score=function(self,score)
		self.score+=score
	end

	obj.add_penalty=function(self)
		sfx(4,3)
		self.penalty+=1
		if self.penalty>=penalty_max then
			game_status="game_over"
		end
	end

	obj.draw=function(self)
		rectfill(0,0,127,draw_min_y,0)
		print("score:" .. self.score
								--.. " penalty:" .. self.penalty
								,0,0,7)

		--local p_mark_x=127-(penalty_max*(8+1))
		local p_mark_x=127-8
		for i=1,penalty_max do
			local color=14
			if i<=self.penalty then
				color=6
			end
			print(penalty_char,p_mark_x,0,color)
			--p_mark_x+=8+1
			p_mark_x-=8+1
		end
	end

	return obj
end

-- actor function ---------------------------------------------------
function is_not_real(actor)
	return(not actor.is_real)
end

function anim_actor(actor)
	if(is_not_real(actor))return

	actor.spr_idx=band(actor.spr_idx,bnot(0x1))
	if(actor.dir_type=="up")actor.spr_idx=bor(actor.spr_idx,0x1)

	actor.t+=1
	if(actor.t%actor.anim_interval<1)then
		switch_foot_actor(actor)
	end
end

function switch_foot_actor(actor)
	if(is_not_real(actor))return

	actor.spr_idx=band(actor.spr_idx,bnot(0x6))
	if(actor.is_r_foot)then
		actor.spr_idx=bor(actor.spr_idx,0x2)
	else
		actor.spr_idx=bor(actor.spr_idx,0x4)
	end
	actor.is_r_foot=not actor.is_r_foot
end

function draw_actor(actor)
	if(is_not_real(actor))return

	local spr_idx=bor(actor.spr_idx,actor.spr_offset)
	if actor.spr_pal ~= nil then
		pal(9, spr_replace_tabe[actor.spr_pal])
	end
	spr(spr_idx,actor.x,actor.y)
	pal()
end

function sort_min_y(acts)
	for i=1,#acts do
		local j=i
		while j>1 and acts[j-1].y>acts[j].y do
			acts[j],acts[j-1]=acts[j-1],acts[j]
			j=j-1
		end
	end
end

-- girls function ---------------------------------------------------
function update_girls(gls)
	update_girls_sight(gls)
	update_girls_ai(gls)
	update_girls_anim(gls)
end

function update_girls_sight(gls)
	foreach(gls,girl_sight_check)
end

function girl_sight_check(gl)
	gl.is_sight=false

	if check_col(pl.col,gl.sight) then
		if is_foundable_pen(pl,gl) then
			if should_change_discovery(gl) then
				gl:set_status("discovery")
			end
			gl.is_sight=true
		else
			if gl.status=="normal" then
				gl:set_status("wonder")
			end
		end
	end
end

function is_foundable_pen(pl,gl)
	if(pl.status~="open") return false
	if(pl.dir_type==gl.dir_type) return false
	return true
end

function should_change_discovery(gl)
	if(gl.status=="discovery") return false
	if(gl.status=="escape") return false
	return true
end

function update_girls_ai(gls)
	foreach(gls,update_status)
	foreach(gls,move_npc)
end

function update_status(gl)
	gl.status_timer+=1

	if gl.status=="wonder" then
		if gl.status_timer>=15 then
				gl:set_status("leav")
		end
	elseif gl.status=="discovery" then
		if gl.status_timer>=60 or
					gl.is_sight==false then
						gl:set_status("escape")
		end

		pt:add_score(1)
	end
end

function move_npc(npc)
	if(not npc.is_real)return

	local speed=npc:get_speed()
	if npc.dir_type=="up" then
		speed*=-1
	end

	npc:move(0,speed)
end

function init_npc(npc)
	if npc.init ~= nil then
		npc:init()
	end
end

function update_npc(npc)
	if npc.update ~= nil then
		npc:update()
	end
end

function draw_debug_npc(npc)
	if npc.draw_debug ~= nil then
		npc:draw_debug()
	end
end

function update_girls_anim(gls)
	foreach(gls,anim_actor)
end

--[[
girls wave
--]]
gl_wave_t=0--countdown
gl_wave_interval=30
gl_wave={1,2,5,6,9,10,13,14}
gl_wave_max=5
function update_girls_wave(gls)
	unreal_if_needed(gls)

	-- limit
	local real_cnt=get_real_girl_count(gls)
	if(real_cnt>=gl_wave_max) return;

	-- timer
	gl_wave_t-=1
	if(gl_wave_t>0) return;

	-- can enable
	gl_wave_t=gl_wave_interval
	local ebl_gl=get_can_enable_girl(gls)
	if(ebl_gl==nil) return;

	-- start girl
	dir_type="up"
	if flr(rnd(2))==1 then
		dir_type="down"
	end
	local y=(dir_type=="up")and 127 or draw_min_y
	local w_idx=flr(rnd(#gl_wave))+1
	local x=gl_wave[w_idx]*8

	ebl_gl:respawn(x,y,dir_type)
end

function get_real_girl_count(gls)
	local cnt=0
	for i,gl in pairs(gls) do
		if(gl.is_real) cnt+=1
	end
	return cnt
end

function unreal_if_needed(gls)
	for i,gl in pairs(gls) do
		if gl.is_real then
			if gl.y<=draw_min_y or gl.y>=127 then
				gl.is_real=false
				if gl.status=="leav" then
					pt:add_penalty()
				end
			end
		end
	end
end

function get_can_enable_girl(gls)
	for i,gl in pairs(gls) do
		if not gl.is_real then
			return gl
		end
	end
	return nil
end

