pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
npc_debug=false
--[[
actor functions
--]]
actor={}
actor.new=function(spr_offset)
	local obj={}
	obj.is_real=false
	obj.x=0
	obj.y=0
	obj.t=0
	obj.spr_idx=0
	obj.spr_offset=spr_offset
	obj.is_r_foot=false
	obj.dir_type="down"
	
	obj.switch_dir=function(self,dir_type)
		self.dir_type=dir_type
	end
	
	return obj
end

girl={}
girl.new=function()
	local obj=actor.new(0x10)
	
	obj.start_move=function(self,x,y,dir_type)
		self.x=x;self.y=y
		self.dir_type=dir_type
		self.is_real=true
	end
	
	return obj
end

--[[
actor functions
--]]
function is_not_real(actor)
	return(not actor.is_real)
end

function anim_actor(actor)
	if(is_not_real(actor))return
	
	actor.spr_idx=band(actor.spr_idx,bnot(0x1))
	if(actor.dir_type=="up")actor.spr_idx=bor(actor.spr_idx,0x1)
	
	actor.t+=1
	if(actor.t%16==0)then
		switch_foot_actor(actor)
	end
end
	
function switch_foot_actor(actor)
	if(is_not_real(actor))return
	
	actor.spr_idx=band(actor.spr_idx,bnot(0x6))
	if(actor.is_r_foot)then
		actor.spr_idx=bor(actor.spr_idx,0x2)
	else
		actor.spr_idx=bor(actor.spr_idx,0x4)
	end
	actor.is_r_foot=not actor.is_r_foot
end

function draw_actor(actor)
	if(is_not_real(actor))return
	
	local spr_idx=bor(actor.spr_idx,actor.spr_offset)
	spr(spr_idx,actor.x,actor.y)		
end

function sort_min_y(acts)
	for i=1,#acts do
		local j=i
		while j>1 and acts[j-1].y>acts[j].y do
			acts[j],acts[j-1]=acts[j-1],acts[j]
			j=j-1
		end
	end
end
--[[
members
--]]
actors={}

pl=actor.new()
pl.is_real=true
add(actors,pl)

gl_cnt=10
gls={}
for i=1,gl_cnt do
	gls[i]=girl.new()
	add(actors,gls[i])
end

--[[
original func
--]]
function _init()
	pl.x=70
	pl.y=70
	init_girls()
	
	debug_init()
end

function _update()
	update_player()
	update_girls_wave()
	update_girls()
end

function _draw()
	rectfill(0,0,127,127,6)
	draw_actors=actors
	sort_min_y(draw_actors)
	foreach(draw_actors,draw_actor)
	--draw_girls(0,pl.y)
	--draw_player()
	--draw_girls(pl.y,127)
end

--[[
girls functions
--]]
function init_girls()
	for i=1,gl_cnt do
		gls[i].x=10*i
		gls[i].y=10*i
	end
end

function update_girls()
	update_girls_ai()
	update_girls_anim()
end

function update_girls_ai()
	foreach(gls,move_girl)
end

girls_move_speed=0.3
function move_girl(girl)
	if(not girl.is_real)return
	
	if girl.dir_type=="up" then
		girl.y-=girls_move_speed
	else
		girl.y+=girls_move_speed
	end
end

function update_girls_anim()
	foreach(gls,anim_actor)
end

function draw_girls(min_y,max_y)
	local spr_offset=0x10
	for i,gl in pairs(gls) do
		if gl.y>=min_y and gl.y<=max_y then
			draw_actor(gl)
		end
	end
end

--[[
girls wave
--]]
gl_wave_t=0--countdown timer
gl_wave_interval=30
gl_wave={0,1,4,5,6,9,10,11,14,15}
function update_girls_wave()
	for i,gl in pairs(gls) do
		if gl.is_real then
			if(gl.y<=0 or gl.y>=127) gl.is_real=false;break 
		end
	end
	
	if gl_wave_t<=0 then
		gl_wave_t=gl_wave_interval
		local ebl_gl=get_can_enable_girl()
		if ebl_gl~=nil then
			dir_type="up"
			if flr(rnd(2))==1 then
				dir_type="down"
			end
			local y=(dir_type=="up")and 127 or 0
			local w_idx=flr(rnd(#gl_wave))+1
			local x=gl_wave[w_idx]*8			
			
			ebl_gl:start_move(x,y,dir_type)
		end
	else
		gl_wave_t-=1
	end 
end

function get_can_enable_girl()
	for i,gl in pairs(gls) do
		if not gl.is_real then
			return gl
		end
	end
	return nil
end

--[[
player functions
--]]
function update_player()
	-- pushed action
	if(btn(4))then
			switch_open()
	-- move
	else
		if(btn(0))pl.x-=1
		if(btn(1))pl.x+=1
		if(btn(2))pl.y-=1;pl:switch_dir("up")
		if(btn(3))pl.y+=1;pl:switch_dir("down")	
		
		update_player_anim()
	end
end

function draw_player()
	draw_actor(pl)
end

function update_player_anim()
	-- if open switch just now
	if(band(pl.spr_idx,0x6)==0x6)switch_foot_actor(pl);return
	anim_actor(pl)
end

function switch_open()
	pl.spr_idx=bor(pl.spr_idx,0x6)
end

--[[
debug
--]]
function debug_switch_npc()
	npc_debug=not npc_debug
end

function debug_init()
	menuitem(1,"npc_debug",
	function()debug_switch_npc()end)
end
