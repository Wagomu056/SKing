-- player function ---------------------------------------------------
function init_player(p)
	p.is_real=true
	p.x=72
	p.y=70
end

function update_player(p)
	-- pushed action
	pre_status=p.status
	p.status=btn(4)and"open"or"normal"

	if pre_status!=p.status then
		if p.status=="open" then
			p.spr_idx=bor(p.spr_idx,0x6)
			sfx(0,1)
		else
			p.spr_idx=band(p.spr_idx,not 0x6)
		end
	end

	-- move
	if p.status=="normal" then
		if btn(2) then
			if not check_wall(p,"up") then
				p.y-=1
				if p.y<=draw_min_y then
					p.y=draw_min_y
				end
			end
			p:change_dir("up")
		elseif btn(3) then
			if not check_wall(p,"down") then
				p.y+=1
			end
			p:change_dir("down")
		end

		if btn(0) then
			if not check_wall(p,"left") then
				p.x-=1
			end
		elseif btn(1) then
			if not check_wall(p,"right") then
				p.x+=1
			end
		end

		anim_actor(p)
	end

	p.col:move(p.x,p.y)
end

function check_wall(pos,dir_type)
	local pos_a,pos_b=get_wall_check_pos(pos,dir_type)

	if(is_wall_map(pos_a)) return true
	if(is_wall_map(pos_b)) return true

	return false
end

function get_wall_check_pos(pos,dir_type)
	--offset base pos
	local offset={
		up   ={x=0, y=-1},
		down ={x=0, y=1},
		right={x=1, y=0},
		left ={x=-1,y=0}
	}

	local base={}
	base.x=pos.x+offset[dir_type].x
	base.y=pos.y+offset[dir_type].y

	--vertex by dir type
	local v=7
	local ver={
		{x=0,y=0},
		{x=v,y=0},
		{x=0,y=v},
		{x=v,y=v}
	}
	local verset={
		up   ={a=ver[1],b=ver[2]},
		down ={a=ver[3],b=ver[4]},
		right={a=ver[2],b=ver[4]},
		left ={a=ver[1],b=ver[3]}
	}

	local a={}
	local b={}
	a.x=base.x+verset[dir_type].a.x
	a.y=base.y+verset[dir_type].a.y
	b.x=base.x+verset[dir_type].b.x
	b.y=base.y+verset[dir_type].b.y

	return a,b
end

function is_wall_map(pos)
	local map_val=mget(pos.x/8,pos.y/8)
	return (fget(map_val,0))
end

-- basic system ---------------------------------------------------
function _init()
	actors={}

	--player
	pl=player.new()
	add(actors,pl)
	init_player(pl)

	--girls
	girls_max=10
	girls={}
	for i=1,girls_max do
		girls[i]=girl.new()
		add(actors,girls[i])
	end

	--old_man
	debug_print=nil
	--[[
	old_mans_max=1
	old_mans={}
	for i=1,old_mans_max do
		old_mans[i]=old_man.new()
		init_old_man(old_mans[i])
		add(actors,old_mans[i])
	end
	--]]

	foreach(actors,init_npc)

	--point
	pt=point.new()

	--twinkle
	twinkles={}
	for i=1,twinkle_num do
		twinkles[i]=twinkle.new()
	end
	init_twinkle(twinkles)

	--game status
	game_status="title"
	is_pushed_button=false

	--debug
	--debug_init()
	--debug_print=nil
end

function _update()
	if game_status=='title' then
		if is_pushed_button==false then
			if btn(4) or btn(5) then
				_init()
				game_status="in_game"
			end
		end

		update_twinkle(twinkles)
	elseif game_status=="in_game" then
		update_old_mans(old_mans)
		update_player(pl)
		update_girls_wave(girls)
		update_girls(girls)
	elseif game_status=="game_over" then
		if is_pushed_button==false then
			if btn(4) or btn(5) then
				_init()
			end
		end
 	end

	is_pushed_button=(btn(4) or btn(5))
end

title_timer = 0
title_is_show = true
function _draw()
	if game_status=='title' then
		-- background
		rectfill(0, 0, 580, 540, 1)

		-- title logo
		local spr_size = 4
		local logo_spr = 128
		local logo_pos_start_up = {16,16}
		local logo_pos_start_low = {16,48}
		local logo_pos_offset = 8 * spr_size
		for i=0,4 do
			if i <= 1 then
				spr(logo_spr+i*spr_size,
					logo_pos_start_up[1]+logo_pos_offset*i,
					logo_pos_start_up[2],
					spr_size,spr_size)
			else
				spr(logo_spr+i*spr_size,
					logo_pos_start_low[1]
					+logo_pos_offset*i-16,
					logo_pos_start_low[2],
					spr_size,spr_size) 
			end
		end

		-- message
		title_timer += 1
		if title_timer >= 30 then
			title_timer = 0
			title_is_show = not title_is_show
		end
		if title_is_show then
			print("\x97 to start",40,96,7)
		end

		draw_twinkle(twinkles)
	else
		map(0,0,0,0,16,16)
		-- draw all actor
		draw_actors=actors
		sort_min_y(draw_actors)
		foreach(draw_actors,draw_actor)

		-- draw score
		pt:draw()

		if game_status=="game_over" then
			rectfill(0,56,127,72,0)
			print("game over",48,58,7)
			print("score:" .. pt.score,48,66,7)
		end
 	end

	-- debug
	--[[
	debug_draw()
	foreach(actors,draw_debug_npc)
	--]]
end

-- utile ---------------------------------------------------
function call_init(obj)
	obj:init()
end

function call_update(obj)
	obj:update()
end

function call_draw(obj)
	obj:draw()
end

-- debug ---------------------------------------------------
view_col=false
function debug_switch_npc()
	view_col=not view_col
end

function debug_init()
	menuitem(1,"view_col",
	function()debug_switch_npc()end)
end

function debug_draw()
	if debug_print~=nil then
		print(debug_print,1,1,5)
	end
	if view_col then
		pl.col:draw(11)
		foreach(girls,draw_girl_sight)
	end
end

function draw_girl_sight(gl)
	if(is_not_real(gl))return
	gl.sight:draw(11)
end
